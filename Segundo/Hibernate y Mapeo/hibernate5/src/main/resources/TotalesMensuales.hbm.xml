<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="modelos.TotalesMensuales" table="totales_mensuales" schema="bd_neptuno">
        <property name="año" column="año"/>
        <property name="mes" column="mes"/>
        <property name="fecha" column="fecha"/>
        <property name="cargo" column="cargo"/>
        <property name="importe" column="importe"/>
        <property name="total" column="total"/>
        <property name="numPedidos" column="num_pedidos"/>
    </class>
</hibernate-mapping>