<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="modelos.Importes" table="importes" schema="bd_neptuno">
        <property name="pedidoId" column="pedido_id"/>
        <property name="productoId" column="producto_id"/>
        <property name="precioUnidad" column="precio_unidad"/>
        <property name="cantidad" column="cantidad"/>
        <property name="descuento" column="descuento"/>
        <property name="importe" column="importe"/>
    </class>
</hibernate-mapping>